#!/usr/bin/env bash 

CONN_SNAP=`snapctl get --slot :certs snapname`
CONN_UUID=`snapctl get --slot :certs uuid`

logger "Deleting GPG-key ${CONN_SNAP}_${CONN_UUID}"

rm -f "${SNAP_DATA}/.keys/${CONN_SNAP}_${CONN_UUID}.pub"

if [Â $? -ne 0 ]; then
    logger "${CONN_SNAP} (uuid: ${CONN_UUID}) did not exist."
fi
